<!-- ‚úÖ Script: compute winner, set bars, show only the right section -->
<script>
document.addEventListener("DOMContentLoaded", function () {
  const params = new URLSearchParams(window.location.search);

  let sheep = parseInt(params.get("sheep")) || 0;
  let love_bird = parseInt(params.get("love_bird")) || 0;
  let owl = parseInt(params.get("owl")) || 0;
  let rhino = parseInt(params.get("rhino")) || 0;

  // Optional override: ?animal=sheep|love_bird|owl|rhino
  const animalOverride = (params.get("animal") || "").trim();
  if (["sheep", "love_bird", "owl", "rhino"].includes(animalOverride)) {
    sheep     = (animalOverride === "sheep") ? 100 : 0;
    love_bird = (animalOverride === "love_bird") ? 100 : 0;
    owl       = (animalOverride === "owl") ? 100 : 0;
    rhino     = (animalOverride === "rhino") ? 100 : 0;
  }

  const firstName = params.get("first_name") || "Leader";

  // Calculate total & percentages
  const total = sheep + love_bird + owl + rhino;
  const sheepPercent = total ? Math.round((sheep / total) * 100) : 0;
  const love_birdPercent = total ? Math.round((love_bird / total) * 100) : 0;
  const owlPercent = total ? Math.round((owl / total) * 100) : 0;
  const rhinoPercent = total ? Math.round((rhino / total) * 100) : 0;

  document.getElementById("score-sheep").textContent = sheepPercent + "%";
  document.getElementById("bar-sheep").style.width = sheepPercent + "%";
  document.getElementById("score-love_bird").textContent = love_birdPercent + "%";
  document.getElementById("bar-love_bird").style.width = love_birdPercent + "%";
  document.getElementById("score-owl").textContent = owlPercent + "%";
  document.getElementById("bar-owl").style.width = owlPercent + "%";
  document.getElementById("score-rhino").textContent = rhinoPercent + "%";
  document.getElementById("bar-rhino").style.width = rhinoPercent + "%";

  // Decide winner with deterministic top-two tiebreak
  const scoresRaw = { sheep, love_bird, owl, rhino };
  const secondaryOrder = ["sheep", "love_bird", "owl", "rhino"];
  const animals = Object.keys(scoresRaw).sort((a,b) => {
    if (scoresRaw[b] !== scoresRaw[a]) return scoresRaw[b] - scoresRaw[a];
    return secondaryOrder.indexOf(a) - secondaryOrder.indexOf(b);
  });
  const topTwo = animals.slice(0,2);
  const topAnimal = (scoresRaw[topTwo[0]] === scoresRaw[topTwo[1]]) ? topTwo[0] : animals[0];

  // Result banner text
  const titles = {
    sheep: "üêë You are a <span class='highlight-block'>Sheep Leader</span>!",
    love_bird: "ü¶ú You are a <span class='highlight-block'>Love Bird Leader</span>!",
    owl: "ü¶â You are an <span class='highlight-block'>Owl Leader</span>!",
    rhino: "ü¶è You are a <span class='highlight-block'>Rhino Leader</span>!"
  };
  const descs = {
    sheep: "Steady Loyalty. Calm Authority. Consistent Results.",
    love_bird: "You‚Äôve risen stronger than ever. Now it‚Äôs your time to dominate.",
    owl: "You have the vision. Now you need the structure to execute it.",
    rhino: "The Power Driver. ‚Ä®You charge forward, make bold calls, and get results fast. But pushing too hard can burn energy, break momentum, and miss the bigger picture."
  };

  let resultText = titles[topAnimal];
  let resultDesc = descs[topAnimal];

  // Fallback if all scores = 0
  if (!sheep && !love_bird && !owl && !rhino) {
    resultText = titles.sheep;
   resultDesc ="Steady Loyalty. Calm Authority. Consistent Results."
;
  }

  document.getElementById("result-title").innerHTML = resultText;
  document.getElementById("result-description").textContent = resultDesc;

  // Greeting
  document.getElementById("greeting").innerHTML =
    `Hi <span class="highlight-name">${firstName}</span>, here's your Leadership Power üëá`;

  // SHOW/HIDE: only show the winning animal section
  document.querySelectorAll(".animal-section").forEach(div => div.style.display = "none");
  const winnerEl = document.getElementById(topAnimal + "-section");
  if (winnerEl) winnerEl.style.display = "block";
});
</script>

